      "When": {
        "HasFile:{{FromFile}}": true
      }
    },
    // MISCELLANEOUS
    {
      "LogName": "Abigail - Emoji",
      "Action": "EditImage",
      "Target": "LooseSprites/emojis",
      "FromFile": "assets/{{NoNose}}/Random/emojis.png",
